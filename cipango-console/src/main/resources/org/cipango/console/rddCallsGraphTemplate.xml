<rrd_graph_def>
     <span>
         <!-- ISO FORMAT: yyyy-MM-dd HH:mm:ss -->
         <start>${start}</start>
         <!-- timestamp in seconds is also allowed -->
         <end>${end}</end>
     </span>
     <options>
         <title>Current Call Sessions</title>
         <image_format>png</image_format>
         <min_value>0</min_value>
         <width>717</width>
         <height>300</height>
     </options>
     <datasources>
         <def>
             <name>calls</name>
             <rrd>${rrd}</rrd>
             <source>calls</source>
             <cf>AVERAGE</cf>
         </def>
     </datasources>
     <graph>
         <line>
             <datasource>calls</datasource>
             <color>#FF0000</color>
             <legend>call sessions\r</legend>
         </line>
         <gprint>
           <datasource>calls</datasource>
           <cf>MAX</cf>
           <format>max call sessions = %7.3f</format>
         </gprint>
         <gprint>
           <datasource>calls</datasource>
           <cf>AVERAGE</cf>
           <format>avg call sessions = %7.3f\r</format>
         </gprint>
     </graph>
</rrd_graph_def>