<rrd_graph_def>
     <span>
         <start>${start}</start>
         <end>${end}</end>
     </span>
     <options>
         <title>SIP Messages</title>
         <vertical_label>msg/s</vertical_label>
         <image_format>png</image_format>
         <min_value>0</min_value>
         <width>717</width>
         <height>300</height>
     </options>
     <datasources>
         <def>
             <name>incomingMessages</name>
             <rrd>${rrd}</rrd>
             <source>incomingMessages</source>
             <cf>AVERAGE</cf>
         </def>
         <def>
             <name>outgoingMessages</name>
             <rrd>${rrd}</rrd>
             <source>outgoingMessages</source>
             <cf>AVERAGE</cf>
         </def>
     </datasources>
     <graph>
       <line>
         <datasource>incomingMessages</datasource>
         <color>#FF0000</color>
         <legend>received sip messages\r</legend>
       </line>
       <gprint>
	       <datasource>incomingMessages</datasource>
	       <cf>MAX</cf>
	       <format>max received = %7.3f msg/s</format>
	     </gprint>
	     <gprint>
	       <datasource>incomingMessages</datasource>
	       <cf>AVERAGE</cf>
	       <format>avg received = %7.3f msg/s\r</format>
	     </gprint>
	     <line>
         <datasource>outgoingMessages</datasource>
         <color>#00FF00</color>
         <legend>sent sip messages\r</legend>
       </line>
       <gprint>
	       <datasource>outgoingMessages</datasource>
	       <cf>MAX</cf>
	       <format>max sent = %7.3f msg/s</format>
	     </gprint>
	     <gprint>
	       <datasource>outgoingMessages</datasource>
	       <cf>AVERAGE</cf>
	       <format>avg sent = %7.3f msg/s\r</format>
	     </gprint>
     </graph>
</rrd_graph_def>