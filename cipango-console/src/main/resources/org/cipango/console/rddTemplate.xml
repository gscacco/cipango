 <rrd_def>
     <path>${path}</path>
     <!-- not mandatory -->
     <start>${start}</start>
     <!-- not mandatory -->
     <step>10</step>
     <!-- at least one datasource must be supplied -->
     <datasource>
         <name>calls</name>
         <type>GAUGE</type>
         <heartbeat>10</heartbeat>
         <min>0</min>
         <max>U</max>
     </datasource>
     <datasource>
         <name>maxMemory</name>
         <type>GAUGE</type>
         <heartbeat>10</heartbeat>
         <min>0</min>
         <max>U</max>
     </datasource>
     <datasource>
         <name>usedMemory</name>
         <type>GAUGE</type>
         <heartbeat>10</heartbeat>
         <min>0</min>
         <max>U</max>
     </datasource>
     <datasource>
         <name>totalMemory</name>
         <type>GAUGE</type>
         <heartbeat>10</heartbeat>
         <min>0</min>
         <max>U</max>
     </datasource>
     <datasource>
         <name>incomingMessages</name>
         <type>COUNTER</type>
         <heartbeat>10</heartbeat>
         <min>0</min>
         <max>U</max>
     </datasource>
     <datasource>
         <name>outgoingMessages</name>
         <type>COUNTER</type>
         <heartbeat>10</heartbeat>
         <min>0</min>
         <max>U</max>
     </datasource>
     <!-- at least one archive must be supplied -->
     <archive>
         <cf>AVERAGE</cf>
         <xff>0.5</xff>
         <!-- 10s -->
         <steps>1</steps>
         <!-- 4 hours -->
         <rows>1440</rows>
     </archive>
     <archive>
         <cf>AVERAGE</cf>
         <xff>0.5</xff>
         <!-- 5 minutes -->
         <steps>30</steps>
         <!-- two days -->
         <rows>576</rows>
     </archive>
     <archive>
         <cf>AVERAGE</cf>
         <xff>0.5</xff>
         <!-- 1 hour -->
         <steps>360</steps>
         <!-- two weeks -->
         <rows>316</rows>
     </archive>
     <archive>
         <cf>MAX</cf>
         <xff>0.5</xff>
         <!-- 20s -->
         <steps>2</steps>
         <!-- 5 minutes -->
         <rows>15</rows>
     </archive>
     <archive>
         <cf>MAX</cf>
         <xff>0.5</xff>
         <!-- 5 minutes -->
         <steps>5</steps>
         <!-- 4 hours -->
         <rows>48</rows>
     </archive>
     <archive>
         <cf>MAX</cf>
         <xff>0.5</xff>
         <!-- 30 minutes -->
         <steps>180</steps>
         <!-- two days -->
         <rows>96</rows>
     </archive>
     <archive>
         <cf>MAX</cf>
         <xff>0.5</xff>
         <!-- 4 hours -->
         <steps>1440</steps>
         <!-- two weeks -->
         <rows>79</rows>
     </archive>
 </rrd_def>