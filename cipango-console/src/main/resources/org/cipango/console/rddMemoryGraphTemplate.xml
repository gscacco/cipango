<rrd_graph_def>
     <span>
         <start>${start}</start>
         <end>${end}</end>
     </span>
     <options>
         <title>JVM memory</title>
         <vertical_label>bytes</vertical_label>
         <image_format>png</image_format>
         <min_value>0</min_value>
         <width>717</width>
         <height>300</height>
     </options>
     <datasources>
         <def>
             <name>max</name>
             <rrd>${rrd}</rrd>
             <source>maxMemory</source>
             <cf>AVERAGE</cf>
         </def>
         <def>
             <name>used</name>
             <rrd>${rrd}</rrd>
             <source>usedMemory</source>
             <cf>AVERAGE</cf>
         </def>
         <def>
             <name>total</name>
             <rrd>${rrd}</rrd>
             <source>totalMemory</source>
             <cf>AVERAGE</cf>
         </def>
         <cdef>
             <name>available</name>
             <rpn>total,used,-</rpn>
         </cdef>
     </datasources>
     <graph>
       <area>
          <datasource>used</datasource>
          <color>#FFFFFF</color>
          <legend/>
       </area>
       <stack>
          <datasource>available</datasource>
          <color>#99FF66</color>
          <legend/>
       </stack>
       <line>
           <datasource>used</datasource>
           <color>#FF0000</color>
           <legend>used memory</legend>
       </line>
       <line>
           <datasource>total</datasource>
           <color>#006600</color>
           <legend>total memory</legend>
       </line>
       <line>
           <datasource>max</datasource>
           <color>#0000FF</color>
           <legend>max memory\r</legend>
       </line>      
        <gprint>
           <datasource>max</datasource>
           <cf>AVERAGE</cf>
           <format>max memory = %7.2f %Sb</format>
         </gprint>
	     <gprint>
	       <datasource>used</datasource>
	       <cf>MAX</cf>
	       <format>max used memory = %7.2f %Sb  </format>
	     </gprint>
	     <gprint>
	       <datasource>used</datasource>
	       <cf>AVERAGE</cf>
	       <format>avg used memory = %7.2f %Sb\r</format>
	     </gprint>
	     <gprint>
	       <datasource>total</datasource>
	       <cf>MAX</cf>
	       <format>max total memory = %7.2f %Sb</format>
	     </gprint>
	     <gprint>
	       <datasource>total</datasource>
	       <cf>AVERAGE</cf>
	       <format>avg total memory = %7.2f %Sb\r</format>
	     </gprint>
     </graph>
</rrd_graph_def>